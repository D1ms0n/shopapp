{
  "name": "react-webpack-babel-kit-2",
  "version": "1.0.0",
  "description": "React Webpack 2.0 babel kit with bootstrap framework",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --inline --history-api-fallback webpack -p --config ./webpack.prod.config.js --progress --colors",
    "start:dev": "webpack-dev-server --config ./webpack.dev.config.js  --progress --colors",
    "build:dev": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack -p --config ./webpack.prod.config.js --progress --colors"
  },
  "author": "author",
  "license": "MIT",
  "devDependencies": {
    "react-scripts": "0.6.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "0.5.8",   
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "css-hot-loader": "1.1.1",
    "css-loader": "0.28.4",
    "isomorphic-fetch": "^2.2.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.3",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-thunk": "2.2.0",
    "react-hot-loader": "3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2"
  },
  "dependencies": {      
    "bootstrap": "4.0.0-beta.3",
    "bootstrap-sass": "3.3.7"  
  }
}
